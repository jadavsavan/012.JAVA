<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Prime</title>
    </head>
    <body>
        <form action="Prime.jsp">
        <input type="text" name="num1"><br><br>
        <input type="text" name="num2"><br><br>
        <input type="submit" value="OK"><br><br>
        </form>
        <%
        if(request.getParameter("num1") == null || request.getParameter("num1") == null){
            out.print("<b>Enter Number</b>");
        }else{
          int low = Integer.parseInt(request.getParameter("num1"));
          int high = Integer.parseInt(request.getParameter("num2"));
          while (low < high) {
            boolean flag = false;
            for(int i = 2; i <= low/2; ++i) {
                if(low % i == 0) {
                    flag = true;
                    break;
                }
            }
            if (!flag)
                out.print(low + ",");
            ++low;
        }
        }
       
        %>
    </body>
</html>
